ty: Type
tm: Type
vl: Type

TNat: ty
TAll: ty -> (vl -> ty) -> ty
TSel: vl -> ty
TSelA : vl -> ty -> ty
TBind : (vl -> ty) -> ty
TSBind: ty -> (vl -> ty) -> ty

tapp: tm -> tm -> tm
